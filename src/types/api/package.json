{
  "name": "@mattermost/desktop-api-types",
  "version": "5.7.0",
  "description": "Shared types for the Desktop App API provided to the Web App",
  "files": [
    "lib"
  ],
  "exports": {
    "./*": "./lib/*.js"
  },
  "typesVersions": {
    ">=3.1": {
      "*": [
        "./lib/*.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "github:mattermost/desktop",
    "directory": "src/types/api"
  },
  "peerDependencies": {
    "typescript": "^4.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsc --build --verbose",
    "run": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf tsconfig.tsbuildinfo ./lib"
  },
  "license": "MIT"
}
